<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>MiraiGo_C</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>embedded</DebugType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>embedded</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EleCho.ConsoleAnsi" Version="1.0.0" />
    <PackageReference Include="EleCho.GoCqHttpSdk" Version="1.0.0-alpha6" />
    <PackageReference Include="EleCho.GoCqHttpSdk.MessageMatching" Version="1.0.0-alpha1" />
    <PackageReference Include="NullLib.CommandLine" Version="1.3.0.3" />
  </ItemGroup>

</Project>
